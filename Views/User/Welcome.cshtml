@model Absence.Models.User

@{
    ViewData["Title"] = "Register";
}


    <h1 class="display-4">Hello @ViewData["Role"]</h1>

    <div class="jumbotron">
      <h1 class="display-4">Hello @ViewData["Username"]</h1>
      <p class="lead">Hey Hey And Welcome Again This is Our Application To Manage Absence </p>
      <hr class="my-4">
      <p>This is Project of DotNet Technologie , Supervised By Profesor Aouatif Amine</p>
      <p class="lead">


        @if(@ViewData["Role"].ToString() == "Admin"){
            <a class="btn btn-primary btn-lg" asp-controller="User" asp-action="AdminDash" role="button">Dashboard</a>
        }else if(@ViewData["Role"].ToString() == "Prof"){
            <a class="btn btn-primary btn-lg" asp-controller="User" asp-action="ProfDash" role="button">Dashboard</a>
        }else{
            <a class="btn btn-primary btn-lg" asp-controller="User" asp-action="StudentDash" role="button">Dashboard</a>
        }

      </p>
    </div>
    
